{
    "name": "kejawenlab/semart-api-gateway",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-redis": "*",
        "pimple/pimple": "^3.0",
        "ramsey/uuid": "^4.0",
        "ruflin/elastica": "^7.0",
        "symfony/console": "^5.1",
        "symfony/dotenv": "^5.1",
        "symfony/http-client": "^5.1",
        "symfony/http-foundation": "^5.1",
        "symfony/http-kernel": "^5.1",
        "symfony/routing": "^5.1",
        "symfony/yaml": "^5.1",
        "webmozart/assert": "^1.9"
    },
    "suggest": {
        "ext-swoole": "To enable Swoole Server",
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "KejawenLab\\SemartApiGateway\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "require-dev": {
        "swoole/ide-helper": "@dev"
    }
}
